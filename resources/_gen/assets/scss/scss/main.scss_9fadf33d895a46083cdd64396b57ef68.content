:root {
  --color-dark: #222;
  --color-body: #373a3c;
  --color-light: #666;
  --color-lighter: #888;
  --color-lightest: #f5f5f5;
  --color-link: #542BFF;
  --color-selection: #EEEAFF;
  --color-accent: #287088;
  --color-white: #fff;
  --font-default: -apple-system,BlinkMacSystemFont,segoe ui,Roboto,Helvetica,Arial,sans-serif,apple color emoji,segoe ui emoji,segoe ui symbol;
  --font-mono: "SF Mono", "Monaco", "Inconsolata", "Fira Mono", "Droid Sans Mono", "Source Code Pro", monospace;
  --font-size-lg: 3.2em;
  --font-size-md: 1.5em;
  --font-size-default: 1.3rem;
  --line-height-lg: 2;
  --line-height-md: 1.5;
  --line-height-sm: 1.4;
  --line-height-xs: 1.2;
  --space: 8px; }

::selection {
  color: var(--color-link);
  background-color: var(--color-selection); }

a {
  color: var(--color-dark); }

a:not([class]) {
  color: var(--color-link); }
  a:not([class]):hover {
    background: var(--color-link);
    color: var(--color-white);
    text-decoration: none; }

a:active,
a:focus {
  outline: dotted 1px var(--color-dark);
  outline-offset: 2px; }

a:not([class]):active,
a:not([class]):focus {
  outline: dotted 1px var(--color-link); }

*,
*::before,
*::after {
  box-sizing: border-box; }

/* Remove default padding and list style */
ul[class],
ol[class] {
  padding: 0;
  list-style: none; }

/* Remove default margin */
body,
h1,
h2,
h3,
h4,
p,
ul[class],
ol[class],
li,
figure,
figcaption,
blockquote,
dl,
dd {
  margin: 0; }

body {
  min-height: 100vh;
  scroll-behavior: smooth;
  text-rendering: optimizeSpeed;
  line-height: 1.5; }

a[class] {
  text-decoration: none; }

a:not([class]) {
  text-decoration: underline;
  text-decoration-skip-ink: auto;
  cursor: pointer; }

img {
  max-width: 100%;
  display: block; }

input,
button,
textarea,
select {
  font: inherit; }

body {
  color: var(--color-body);
  font-family: var(--font-default);
  font-size: var(--font-size-default); }

h1,
h2,
h3,
h4,
h5,
h6 {
  color: var(--color-dark); }

h1 {
  font-size: var(--font-size-lg);
  line-height: var(--line-height-xs);
  margin: calc(var(--space) *8) 0; }

h2 {
  font-weight: 400;
  font-size: var(--font-size-md);
  line-height: var(--line-height-sm); }

h3 {
  font-size: var(--font-size-md);
  line-height: var(--line-height-sm); }

code {
  font-family: var(--font-mono);
  font-size: 16px; }

p > code {
  background: var(--color-lightest);
  color: var(--color-accent);
  font-size: 93%;
  padding: 4px; }

body {
  margin: 0 auto;
  max-width: 840px;
  padding: calc(var(--space) *6) calc(var(--space) *3); }

main {
  margin: calc(var(--space) *18) 0; }
  main * + * {
    margin-top: calc(var(--space) *4); }
  main section {
    margin: calc(var(--space) *6) 0; }
  main p,
  main p + p {
    max-width: 65ch;
    margin-top: calc(var(--space) *2); }
  main li {
    margin: 0; }
  main li[class]:not(:first-of-type) {
    margin-top: calc(var(--space) *2); }

nav ul {
  display: flex;
  justify-content: space-between; }
  nav ul li:not(:first-of-type) {
    margin-left: calc(var(--space) *2); }

.SiteHead,
footer {
  display: flex;
  justify-content: space-between; }

.Button {
  border: dotted 1px var(--color-link);
  color: var(--color-link);
  padding: var(--space);
  text-decoration: none; }
  .Button:hover {
    border: solid 1px var(--color-link); }

/* Background */
