/*------------------------------------*\
    #COMPONENTS: IMAGE
\*------------------------------------*/
/**
 * Component Block
 */
.Image figcaption {
  line-height: 20px;
  margin-top: var(--space); }

.Image + p,
p + .Image {
  margin-top: calc(var(--space) *2); }

/*------------------------------------*\
    #COMPONENTS: RATIO
\*------------------------------------*/
/**
 * A utility that provides you with a predictable aspect ratio
 *
 * 16:9: ratio wide
 * 1:1: ratio square
 * 3:2: ratio classic
 */
.Ratio {
  background-color: #f6f6f6;
  display: block;
  position: relative; }

.Ratio--landscape {
  padding-bottom: 66.66667%; }

.Ratio--portrait {
  padding-bottom: 133.33333333333334%; }

.Ratio--square {
  padding-bottom: 100%; }

.Ratio img {
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0; }

/*------------------------------------*\
    #COMPONENTS: LAZY
\*------------------------------------*/
/* Animate the lazyloading */
.lazyload,
.lazyloading {
  opacity: 0; }

.lazyloaded {
  opacity: 1;
  transition: opacity 300ms; }

